<div class="panel panel-info">
  <div class="panel-heading">
    <h3> Head to Head User Prediction </h3>
  </div>

  <div class="panel-body">
    <div class="row">
      <div class="col-xs-10 col-xs-offset-1">
        <table class="table table-bordered datatable-nopaging">
          <thead>
            <tr>
              <th><%= @teams[0] %></th>
              <th><%= @teams[1] %></th>
            </tr>
          </thead>

          <tbody>
            <% @result.results.each do |row| %>
              <tr>
                <td><%= row[0].to_i %></td>
                <td><%= row[1].to_i %></td>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>

  </div>
</div>
